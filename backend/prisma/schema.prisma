

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paciente {
  id        String   @id @default(cuid())
  nome      String
  cpf       String   @unique
  idade     Int
  email     String   
  telefone  String   
  endereco  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
